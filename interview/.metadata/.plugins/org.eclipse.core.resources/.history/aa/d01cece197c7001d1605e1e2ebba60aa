package com.bithub;

public class Multiple5 {
	public static void main(String[] args) {
		int arr[] = {3,4,6,3,5,15,423,85,90,21,35,76};
		
		for(int i=0;i<arr.length;i++) {
			if(arr[i]%5==0) {
				int temp = arr[i];
				for(int j=i+1;j<arr.length;j++) {
					arr[i] = arr[j];
					
				}				
			}
		}
	}
}
