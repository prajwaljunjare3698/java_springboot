package com.programs;

import java.time.LocalDate;

class BookTicket{
	private int no;
	private String destination;
	private LocalDate date;
	private int seatNo;
	public BookTicket(int no, String destination, LocalDate date, int seatNo) {
		super();
		this.no = no;
		this.destination = destination;
		this.date = date;
		this.seatNo = seatNo;
	}
	@Override
	public String toString() {
		return "BookTicket [no=" + no + ", destination=" + destination + ", date=" + date + ", seatNo=" + seatNo + "]";
	}
}
class Passenger{
	private String name;
	private int age;
	private String gender;
	private int seatNo;
	public Passenger(String name, int age, String gender, int seatNo) {
		super();
		this.name = name;
		this.age = age;
		this.gender = gender;
		this.seatNo = seatNo;
	}	
}
class Train{
	private int trainNo;

	public Train(int trainNo) {
		super();
		this.trainNo = trainNo;
	}
	
}
