<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspider.springcorexml.manytomany.bean.CustomerBean" name="customer1">
		<property name="id" value="1"/>
		<property name="name" value="Jethalal"/>
		<property name="products">	
			<list>
				<bean class="com.jspider.springcorexml.manytomany.bean.ProductBean" name="product1">
					<property name="id" value="1"/>
					<property name="name" value="T.V."/>
				</bean>
				<bean class="com.jspider.springcorexml.manytomany.bean.ProductBean" name="product2">
					<property name="id" value="2"/>
					<property name="name" value="Freez"/>
				</bean>
			</list>
		</property>
	</bean>
	
	<bean class="com.jspider.springcorexml.manytomany.bean.CustomerBean" name="customer2">
		<property name="id" value="2"/>
		<property name="name" value="iyyer"/>
		<property name="products">	
			<list>
				<bean class="com.jspider.springcorexml.manytomany.bean.ProductBean" name="product1">
					<property name="id" value="1"/>
					<property name="name" value="T.V."/>
				</bean>
				<bean class="com.jspider.springcorexml.manytomany.bean.ProductBean" name="product2">
					<property name="id" value="2"/>
					<property name="name" value="Freez"/>
				</bean>
			</list>
		</property>
	</bean>

</beans>